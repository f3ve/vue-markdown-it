  "devDependencies": {
    "@types/markdown-it": "^13.0.2",
    "@types/node": "^20.8.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.50.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "vite": "^4.4.10",
